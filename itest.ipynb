    "import csv\n",
    "csvFile = open(\"data/HDPE/train_data.csv\",'w',newline='',encoding='utf-8')\n",
    "writer = csv.writer(csvFile)\n",
    "csvRow = []\n",
    "\n",
    "f = open(\"data/HDPE/train_data.txt\",'r',encoding='GB2312')\n",
    "for line in f:\n",
    "    csvRow = line.split()\n",
    "    writer.writerow(csvRow)\n",
    "\n",
    "f.close()\n",
    "csvFile.close()"
   ],
